<h1>会員編集</h1>

	<%= form_for(@customer) do |f| %>

		<h4>画像</h4>

		<h4>カナ</h4>
			<%= f.text_field :last_name_kana %><%= f.text_field :first_name_kana %>
		<h4>名前</h4>
			<%= f.text_field :last_name %><%= f.text_field :first_name %>
		<h4>郵便番号</h4>
			<%= f.text_field :customers_postal_code %>
		<h4>住所</h4>
			<%= f.text_field :customers_address %>
		<h4>電話番号</h4>
			<%= f.text_field :phone_number %>
		<h4>メールアドレス</h4>
			<%= f.text_field :email %>
		<h4>パスワード</h4>
			<%= f.text_field :encrypted_password %>

		<%= f.submit '保存' %>

	<% end %>

